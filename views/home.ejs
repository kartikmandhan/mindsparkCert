<%- include('partials/header') %>
<div class="row d-flex align-items-center justify-content-center">
    <div class="jumbotron jumbotron-fluid">
        <h1 class="display-4 ">Get Mindspark 2019-20 Certificate !</h1>
    </div>
    <div class="col-md-6">
        <div class="login-div mb-5 mx-auto">
            <form id="MSform" action="/" method="post">
                <div class="illustration">
                    <i class="far fa-user"></i>
                </div>

                <div class="fields">
                    <div class="input">
                        <input type="text" name="fName" class="user-input" autocomplete="off" placeholder="First Name"
                            required />
                    </div>
                    <div class="input">
                        <input type="text" name="lName" class="user-input" autocomplete="off" placeholder="Last Name"
                            required />
                    </div>
                    <div class="input">
                        <select type="text" class="user-input" name="portfolio" autocomplete="off" id="portfolioInput"
                            placeholder="Portfolio" required />
                        <option selected="selected" disabled hidden>Select Portfolio</option>
                        <option>Web</option>
                        <option>Technical Championship</option>
                        <option>Events</option>
                        <option>Public Relations and Outreach</option>
                        <option>Refreshments</option>
                        <option>Design</option>
                        <option>Refreshments</option>
                        </select>
                    </div>
                </div>
                <button type="submit" class="signin-button">Get Certificate</button>

            </form>
        </div>


    </div>
</div>

